<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.3"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
>
	<application>
		<!--
		Builtin JSF Messages.properties locales in Wildfly 19.1
		$AP3_JBOSS_DIR/modules/system/layers/base/javax/faces/api/main/jboss-jsf-api_2.3_spec-3.0.0.SP02.jar
		de  	German
		en  	English
		es  	Spanish
		fr  	French
		ja  	Japanese
		ko  	Korean
		pt_BR	Portuguese (Brazil)
		zh-CN	Chinese (Simplified)
		zh-HK	Chinese (Hong Kong)
		zh-TW	Chinese (Taiwan)
		-->
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>de</supported-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>es</supported-locale>
			<supported-locale>fr</supported-locale>
		</locale-config>

		<!-- Root of classpath, /src/main/java/Messages.properties -->
		<message-bundle>Messages</message-bundle>
	</application>
</faces-config>
